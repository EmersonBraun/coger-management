{
  "name": "management",
  "version": "0.7.1",
  "private": true,
  "scripts": {
    "build": "node ace build",
    "migrate": "node ace migration:run",
    "migration:rollback": "node ace migration:rollback",
    "migration:status": "node ace migration:status",
    "seed": "node ace db:seed",
    "routes": "node ace list:routes",
    "serve": "node ace serve --watch",
    "lint": "eslint . --ext=.ts",
    "release:major": "standard-version --release-as major",
    "release:minor": "standard-version --release-as minor",
    "release:patch": "standard-version --release-as patch",
    "dev": "node node_modules/cross-env/src/bin/cross-env.js NODE_ENV=development webpack --progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js",
    "watch": "node node_modules/cross-env/src/bin/cross-env.js NODE_ENV=development webpack --watch --progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js",
    "hot": "node node_modules/cross-env/src/bin/cross-env.js NODE_ENV=development webpack-dev-server --inline --hot --config=node_modules/laravel-mix/setup/webpack.config.js",
    "vue-production": "node node_modules/cross-env/src/bin/cross-env.js NODE_ENV=production webpack --progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js"
  },
  "devDependencies": {
    "@adonisjs/assembler": "^2.1.5",
    "@fortawesome/fontawesome-free": "^5.15.0",
    "adonis-preset-ts": "^1.0.4",
    "deepmerge": "^4.2.2",
    "eslint": "^7.9.0",
    "eslint-plugin-adonis": "^1.0.15",
    "fibers": "^5.0.0",
    "pino-pretty": "^4.2.1",
    "sass": "^1.26.11",
    "sass-loader": "^10.0.2",
    "typescript": "^4.0.3",
    "vue-template-compiler": "^2.6.12",
    "youch": "^2.0.10",
    "youch-terminal": "^1.0.0"
  },
  "dependencies": {
    "@adonisjs/ace": "^6.9.4",
    "@adonisjs/core": "^5.0.0-preview-rc-1.12",
    "@adonisjs/fold": "^6.4.0",
    "@adonisjs/lucid": "^8.4.2",
    "@adonisjs/session": "^3.1.2",
    "@adonisjs/view": "^2.0.9",
    "axios": "^0.20.0",
    "cross-env": "^7.0.2",
    "laravel-mix": "^5.0.5",
    "luxon": "^1.25.0",
    "pg": "^8.3.3",
    "proxy-addr": "^2.0.6",
    "reflect-metadata": "^0.1.13",
    "source-map-support": "^0.5.19",
    "sqlite3": "^5.0.0",
    "standard-version": "^9.0.0",
    "vue": "^2.6.12",
    "vue-router": "^3.4.4",
    "vuetify": "^2.3.10"
  }
}
